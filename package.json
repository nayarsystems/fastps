{
  "name": "fastps",
  "version": "2.0.1",
  "description": "Simple pub/sub",
  "author": "Angel Jimenez <ajimenez@nayarsystems.com>",
  "license": "MIT",
  "main": "./dist/fastps.js",
  "typings": "./dist/types.d.ts",
  "repository": "https://github.com/nayarsystems/fastps",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "flow": "flow",
    "generatedts": "./node_modules/.bin/jsdoc -t node_modules/tsd-jsdoc/dist -r src -d dist && sed -i 's/declare /export /' dist/types.d.ts",
    "prepublishOnly": "./node_modules/.bin/eslint src/ && npm run test && rm -f dist/* && npm run generatedts && npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.82.0",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "merge": ">=1.2.1",
    "prettier": "^1.14.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "tsd-jsdoc": "^2.1.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/coverage/**",
      "!**/flow-typed/**",
      "!webpack.config.js",
      "!.eslintrc.js"
    ]
  }
}
