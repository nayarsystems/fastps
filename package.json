{
  "name": "fastps",
  "version": "2.0.10",
  "description": "Simple pub/sub",
  "author": "Angel Jimenez <ajimenez@nayarsystems.com>",
  "license": "MIT",
  "main": "dist/fastps.js",
  "typings": "dist/types.d.ts",
  "repository": "https://github.com/nayarsystems/fastps",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "generatedoc": "jsdoc -r src -d dist",
    "prepublishOnly": "eslint src/ && npm run test && rm -r f dist/* && npm run generatedoc && npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "jsdoc": "github:jsdoc/jsdoc",
    "merge": ">=1.2.1",
    "prettier": "^1.18.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/coverage/**",
      "!.eslintrc.js",
      "!webpack.config.js"
    ]
  }
}
